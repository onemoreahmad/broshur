<div class="hidden md:flex absolute rtl:-right-16 ltr:-left-16 flex-col gap-1.5 p-1 z-20 bg-black/20  rounded-xl">
    {% for link in links %}
        <a href="{{link.link}}" target="_blank" class="flex items-center  text-white/80 p-2  rounded-xl bg-white/5  hover:bg-white/10">
            <span class="size-6">{% include 'assets/icons/social/brand-'~data_get(link, 'meta.network')~'.svg' ignore missing %}   </span>
        </a>
    {% endfor %}
</div>
 